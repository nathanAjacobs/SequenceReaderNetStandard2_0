<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Title>SequenceReaderNetStandard2_0</Title>
    <Authors>Nathan Jacobs</Authors>
    <Description>SequenceReader for .NET Standard 2.0</Description>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/nathanAjacobs/SequenceReaderNetStandard2_0</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nathanAjacobs/SequenceReaderNetStandard2_0</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BinaryPrimitivesNetStandard2_0" Version="1.0.0" />
    <PackageReference Include="System.Memory" Version="4.5.5" />
  </ItemGroup>

</Project>
